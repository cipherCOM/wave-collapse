{
  "name": "wave-collapse",
  "description": "A wave function collapse implementation in JavaScript. Inspired by https://github.com/mxgmn/WaveFunctionCollapse and https://github.com/kchapelier/wavefunctioncollapse",
  "version": "0.1.0",
  "author": "Patrik Schmittat",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ciphercom/wave-collapse.git"
  },
  "bugs": {
    "url": "https://github.com/ciphercom/wave-collapse/issues"
  },
  "keywords": [
    "wave",
    "function",
    "collapse",
    "wfc",
    "pattern",
    "generation",
    "procedural",
    "tilemap",
    "tile",
    "map",
    "dungeon",
    "maze",
    "sprite",
    "spritesheet"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "dependencies": {
    "heap-js": "2.5.0"
  },
  "devDependencies": {
    "0x": "5.7.0",
    "prettier": "3.3.2",
    "tsup": "8.1.0",
    "typescript": "5.5.2",
    "vitest": "1.6.0"
  },
  "scripts": {
    "dev": "npm run build-fast -- --watch",
    "build": "tsup src/*.ts --clean",
    "prepublishOnly": "npm run build",
    "test": "npm run build && npm run test-only",
    "format": "prettier --write .",
    "test-only": "vitest run",
    "benchmark": "vitest bench",
    "build-fast": "npm run build -- --no-dts"
  },
  "files": [
    "/dist",
    "/package.json",
    "/LICENSE",
    "/README.md"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  }
}
